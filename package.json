{
  "name": "erc20-balance",
  "version": "0.1.6",
  "description": "Get ERC-20 token balances with JavaScript. Supports Node.js and Deno",
  "exports": {
    "./": "./",
    "./dist/list.json": "./dist/list.json",
    ".": "./dist/index.js"
  },
  "types": "./dist/index.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "node --loader ts-node/esm --experimental-specifier-resolution=node node_modules/uvu/bin.js",
    "test:coverage": "c8 --include=src pnpm test",
    "test:report": "c8 report --reporter=text-lcov > coverage.lcov",
    "prepare": "pnpm build && pnpm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/talentlessguy/erc20-balance.git"
  },
  "keywords": [
    "crypto",
    "erc20",
    "token",
    "blockchain",
    "ethereum"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/talentlessguy/erc20-balance/issues"
  },
  "homepage": "https://github.com/talentlessguy/erc20-balance#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.1",
    "@tinyhttp/dotenv": "^1.3.0",
    "@types/node": "^15.0.3",
    "c8": "^7.7.2",
    "node-fetch": "^2.6.1",
    "rollup": "^2.48.0",
    "rollup-plugin-copy-assets": "^2.0.3",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "uvu": "^0.5.1"
  },
  "engines": {
    "node": ">=12.4"
  }
}
